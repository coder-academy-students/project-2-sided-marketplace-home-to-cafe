<p id="notice"><%= notice %></p>

<h1>Home Baker Profiles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Logo url</th>
      <th>Name</th>
      <th>Address</th>
      <th>Phone number</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @home_baker_profiles.each do |home_baker_profile| %>
      <tr>
        <td><%= home_baker_profile.user %></td>
        <td><%= home_baker_profile.logo_url %></td>
        <td><%= home_baker_profile.name %></td>
        <td><%= home_baker_profile.address %></td>
        <td><%= home_baker_profile.phone_number %></td>
        <td><%= home_baker_profile.latitude %></td>
        <td><%= home_baker_profile.longitude %></td>
        <td><%= link_to 'Show', home_baker_profile %></td>
        <td><%= link_to 'Edit', edit_home_baker_profile_path(home_baker_profile) %></td>
        <td><%= link_to 'Destroy', home_baker_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Home Baker Profile', new_home_baker_profile_path %>
